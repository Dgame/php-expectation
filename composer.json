{
    "name": "dgame/php-expectation",
    "description": "php ensurance",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-expectation",
    "keywords": [
        "Ensurance",
        "Enforcement",
        "Assertion",
        "Design by contract"
    ],
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-expectation"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Expectation\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11.12"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
